package com.example.tests;

import java.util.regex.Pattern;
import java.util.concurrent.TimeUnit;
import org.junit.*;
import static org.junit.Assert.*;
import static org.hamcrest.CoreMatchers.*;
import org.openqa.selenium.*;
import org.openqa.selenium.firefox.FirefoxDriver;
import org.openqa.selenium.support.ui.Select;

public class RecordVitalSigns453 {
  private WebDriver driver;
  private String baseUrl;
  private boolean acceptNextAlert = true;
  private StringBuffer verificationErrors = new StringBuffer();

  @Before
  public void setUp() throws Exception {
    driver = new FirefoxDriver();
    baseUrl = "https://coms.vacloud.us/";
    driver.manage().timeouts().implicitlyWait(30, TimeUnit.SECONDS);
  }

  @Test
  public void testRecordVitalSigns453() throws Exception {
    driver.get(baseUrl + "/index.php");
    driver.findElement(By.id("textfield-1023-inputEl")).clear();
    driver.findElement(By.id("textfield-1023-inputEl")).sendKeys("f0400");
    driver.findElement(By.id("ext-gen2666")).click();
    driver.findElement(By.id("ext-gen2716")).click();
    driver.findElement(By.id("ext-gen2724")).click();
    driver.findElement(By.id("tab-1513-btnEl")).click();
    driver.findElement(By.id("textfield-1170-inputEl")).clear();
    driver.findElement(By.id("textfield-1170-inputEl")).sendKeys("98.2");
    driver.findElement(By.id("ext-gen2803")).click();
    driver.findElement(By.xpath("//div[@id='boundlist-2027-listEl']/ul/li")).click();
    driver.findElement(By.id("textfield-1181-inputEl")).clear();
    driver.findElement(By.id("textfield-1181-inputEl")).sendKeys("70");
    driver.findElement(By.id("textfield-1188-inputEl")).clear();
    driver.findElement(By.id("textfield-1188-inputEl")).sendKeys("176");
    driver.findElement(By.id("textfield-1174-inputEl")).clear();
    driver.findElement(By.id("textfield-1174-inputEl")).sendKeys("74");
    driver.findElement(By.id("textfield-1184-inputEl")).clear();
    driver.findElement(By.id("textfield-1184-inputEl")).sendKeys("19");
    driver.findElement(By.id("numberfield-1191-inputEl")).clear();
    driver.findElement(By.id("numberfield-1191-inputEl")).sendKeys("3");
    driver.findElement(By.id("textfield-1176-inputEl")).click();
    driver.findElement(By.id("textfield-1176-inputEl")).clear();
    driver.findElement(By.id("textfield-1176-inputEl")).sendKeys("118");
    driver.findElement(By.id("textfield-1178-inputEl")).clear();
    driver.findElement(By.id("textfield-1178-inputEl")).sendKeys("82");
    driver.findElement(By.id("numberfield-1185-inputEl")).clear();
    driver.findElement(By.id("numberfield-1185-inputEl")).sendKeys("98");
    driver.findElement(By.id("button-1196-btnEl")).click();
    driver.findElement(By.id("button-1006-btnEl")).click();
    driver.findElement(By.id("button-1005-btnEl")).click();
  }

  @After
  public void tearDown() throws Exception {
    driver.quit();
    String verificationErrorString = verificationErrors.toString();
    if (!"".equals(verificationErrorString)) {
      fail(verificationErrorString);
    }
  }

  private boolean isElementPresent(By by) {
    try {
      driver.findElement(by);
      return true;
    } catch (NoSuchElementException e) {
      return false;
    }
  }

  private boolean isAlertPresent() {
    try {
      driver.switchTo().alert();
      return true;
    } catch (NoAlertPresentException e) {
      return false;
    }
  }

  private String closeAlertAndGetItsText() {
    try {
      Alert alert = driver.switchTo().alert();
      String alertText = alert.getText();
      if (acceptNextAlert) {
        alert.accept();
      } else {
        alert.dismiss();
      }
      return alertText;
    } finally {
      acceptNextAlert = true;
    }
  }
}
