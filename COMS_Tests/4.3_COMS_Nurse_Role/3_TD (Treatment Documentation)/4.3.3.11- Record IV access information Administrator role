package com.example.tests;

import java.util.regex.Pattern;
import java.util.concurrent.TimeUnit;
import org.junit.*;
import static org.junit.Assert.*;
import static org.hamcrest.CoreMatchers.*;
import org.openqa.selenium.*;
import org.openqa.selenium.firefox.FirefoxDriver;
import org.openqa.selenium.support.ui.Select;

public class RecordIVAccessInformation433 {
  private WebDriver driver;
  private String baseUrl;
  private boolean acceptNextAlert = true;
  private StringBuffer verificationErrors = new StringBuffer();

  @Before
  public void setUp() throws Exception {
    driver = new FirefoxDriver();
    baseUrl = "https://coms.vacloud.us/";
    driver.manage().timeouts().implicitlyWait(30, TimeUnit.SECONDS);
  }

  @Test
  public void testRecordIVAccessInformation433() throws Exception {
    driver.get(baseUrl + "/index.php");
    driver.findElement(By.id("tab-1513-btnWrap")).click();
    driver.findElement(By.id("tab-1492-btnEl")).click();
    driver.findElement(By.id("ext-gen4208")).click();
    driver.findElement(By.cssSelector("#ext-gen4240 > a.x-datepicker-date > em > span")).click();
    driver.findElement(By.id("ext-gen4210")).click();
    driver.findElement(By.xpath("//div[@id='boundlist-2064-listEl']/ul/li")).click();
    driver.findElement(By.id("ext-gen4213")).click();
    driver.findElement(By.xpath("//div[@id='boundlist-2066-listEl']/ul/li[5]")).click();
    driver.findElement(By.id("ext-gen4216")).click();
    driver.findElement(By.xpath("//div[@id='boundlist-2068-listEl']/ul/li[8]")).click();
    driver.findElement(By.id("ext-gen4219")).click();
    driver.findElement(By.xpath("//div[@id='boundlist-2070-listEl']/ul/li")).click();
    driver.findElement(By.id("button-1268-btnEl")).click();
    driver.findElement(By.id("button-1005-btnEl")).click();
  }

  @After
  public void tearDown() throws Exception {
    driver.quit();
    String verificationErrorString = verificationErrors.toString();
    if (!"".equals(verificationErrorString)) {
      fail(verificationErrorString);
    }
  }

  private boolean isElementPresent(By by) {
    try {
      driver.findElement(by);
      return true;
    } catch (NoSuchElementException e) {
      return false;
    }
  }

  private boolean isAlertPresent() {
    try {
      driver.switchTo().alert();
      return true;
    } catch (NoAlertPresentException e) {
      return false;
    }
  }

  private String closeAlertAndGetItsText() {
    try {
      Alert alert = driver.switchTo().alert();
      String alertText = alert.getText();
      if (acceptNextAlert) {
        alert.accept();
      } else {
        alert.dismiss();
      }
      return alertText;
    } finally {
      acceptNextAlert = true;
    }
  }
}
